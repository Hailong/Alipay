{
    "name": "payum/alipay",
    "type": "project",
    "description": "The Payum extension. Rapid extensino development",
    "keywords": ["payment"],
    "homepage": "https://payum.forma-pro.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Kotlyar Maksim",
            "email": "kotlyar.maksim@gmail.com"
        },
        {
            "name":     "Payum project",
            "homepage": "https://payum.forma-pro.com/"
        },
        {
            "name":     "Community contributions",
            "homepage": "https://github.com/Payum/AuthorizeNetAim/contributors"
        }
    ],
    "require": {
        "payum/core": "^1.5"
    },
    "require-dev": {
        "payum/core": "^1.5@dev",
        "php-http/guzzle6-adapter": "^1.0"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-0": { "Payum\\Alipay\\Wap": "Wap" },
        "classmap": [
            "AlipaySDK/alipay-sdk-PHP-20180104135052/aop/",
            "AlipaySDK/alipay.trade.page.pay-PHP-UTF-8/pagepay/buildermodel/",
            "AlipaySDK/alipay.trade.wap.pay-PHP-UTF-8/wappay/buildermodel/"
        ],
    },
    "target-dir": "Payum/Alipay",
    "extra": {
        "branch-alias": {
            "dev-master": "1.5-dev"
        }
    }
}
